def implikacja(zmiennaP, zmiennaQ):
    if zmiennaP==0 or zmiennaP==1 and zmiennaQ==0 or zmiennaQ==1:
        if zmiennaP==1 and zmiennaQ==0:
            return False
        else:
            return True
print("Pierwszy argument:")
argumentpierwszy = int(input())
print("Drugi argument:")
argumentdrugi = int(input())
wynik = implikacja(argumentpierwszy, argumentdrugi)
print("Wynik implikacji:", wynik)
